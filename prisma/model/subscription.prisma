model Subscription {
  id                   String    @id @default(uuid())
  plan                 String
  referenceId          String
  stripeCustomerId     String?
  stripeSubscriptionId String?
  status               String
  periodStart          DateTime?
  periodEnd            DateTime?
  cancelAtPeriodEnd    Boolean?
  cancelAt             DateTime?
  canceledAt           DateTime?
  endedAt              DateTime?
  seats                Int?
  trialStart           DateTime?
  trialEnd             DateTime?
}
